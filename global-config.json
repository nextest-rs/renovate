{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended"
    ],
    "platform": "github",
    "repositories": [
        "guppy-rs/guppy",
        "nextest-rs/datatest-stable",
        "nextest-rs/future-queue",
        "nextest-rs/mukti",
        "nextest-rs/nexlint",
        "nextest-rs/nextest",
        "nextest-rs/quick-junit",
        "nextest-rs/renovate"
    ],
    "allowedPostUpgradeCommands": [
        "^scripts/renovate-post-upgrade.sh$"
    ],
    "onboarding": false
}
